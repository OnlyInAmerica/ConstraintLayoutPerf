<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    android:id="@+id/activity_main"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="pro.dbro.constraintlayout.ConstraintActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Started cutting in not just class, I was comin all fast, I was new to the game"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="91dp"
        android:id="@+id/textView10"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        tools:layout_constraintTop_creator="0"
        app:layout_constraintRight_toRightOf="@+id/activity_main"
        tools:layout_constraintRight_creator="0"/>

    <TextView
        android:text="And every Saturday morning"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        android:id="@+id/textView"
        android:textSize="22sp"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toTopOf="@+id/activity_main"
        tools:layout_constraintTop_creator="0"
        app:layout_constraintRight_toLeftOf="@+id/textView7"
        tools:layout_constraintRight_creator="0"
        app:layout_constraintHorizontal_bias="0.0"/>

    <TextView
        android:text="I watched cartoons with a bowl of Frosted Flakes"
        android:layout_width="83dp"
        android:layout_height="91dp"
        tools:layout_editor_absoluteX="301dp"
        tools:layout_editor_absoluteY="0dp"
        android:id="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/activity_main"
        tools:layout_constraintTop_creator="0"
        app:layout_constraintRight_toRightOf="@+id/activity_main"
        tools:layout_constraintRight_creator="0"/>

    <TextView
        android:text="Then.. the puterty came, started hitting them cuties with game and the truancy came"
        android:layout_width="0dp"
        android:layout_height="33dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="42dp"
        android:id="@+id/textView8"
        android:textSize="14sp"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:layout_constraintTop_creator="0"
        app:layout_constraintRight_toLeftOf="@+id/textView7"
        tools:layout_constraintRight_creator="0"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        tools:layout_constraintBottom_creator="0"
        app:layout_constraintHorizontal_bias="0.0"/>

    <TextView
        android:text="Used to playin on T.V"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="124dp"
        android:id="@+id/textView9"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:layout_constraintTop_creator="0"/>

    <ImageView
        android:src="@android:drawable/btn_plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="139dp"
        tools:layout_editor_absoluteY="124dp"
        android:id="@+id/imageView2"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        android:layout_marginStart="8dp"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:layout_constraintTop_creator="0"/>

    <TextView
        android:text="courtesy of video music box"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="124dp"
        android:id="@+id/textView11"
        android:textSize="20sp"
        app:layout_constraintLeft_toRightOf="@+id/imageView2"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:layout_constraintTop_creator="0"
        app:layout_constraintRight_toRightOf="@+id/activity_main"
        tools:layout_constraintRight_creator="0"/>

</android.support.constraint.ConstraintLayout>
